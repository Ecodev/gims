{
    "name": "ecodev/gims",
    "description": "GIMS",
    "homepage": "http://www.gimsinitiative.org",
    "license": "MIT",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/PowerKiKi/PHPExcel"
        },
        {
            "type": "vcs",
            "url": "https://github.com/PowerKiKi/dbal"
        },
        {
            "type": "vcs",
            "url": "https://github.com/PowerKiKi/doctrine2"
        }
    ],
    "autoload": {
        "psr-0": {
